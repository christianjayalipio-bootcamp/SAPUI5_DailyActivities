<mvc:View controllerName="com.ui5.trng.project1.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <footer>
            <Bar>
                <contentLeft>
                    <Button icon="sap-icon://message-popup" text="Message" press="onButton1Press"  />
                </contentLeft>
                <contentRight>
                    <Button icon="sap-icon://navigation-right-arrow"  text="Next" press="onNavPress" iconFirst="false" />
                </contentRight>
            </Bar>
        </footer>
    </Page>
</mvc:View>